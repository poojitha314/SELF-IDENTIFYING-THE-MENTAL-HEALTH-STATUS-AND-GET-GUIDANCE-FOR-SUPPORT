<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>IDYLLIC</title>
    <link rel="stylesheet" href="log.css">
</head>
<body>
    <header>
        <h1>IDYLLIC</h1>
    </header>
    <main>
        <section class="menu">
            <article class="menu-item">
                <div id="container1">
                    <img src="mt.jpeg" height="150px" width="150" alt="Click me" id="image1">
                </div>
                <h2>MOOD Tracker</h2>
                <script>
                    // JavaScript to handle the click event for Mood Tracker
                    document.getElementById('image1').addEventListener('click', function() {
                        // Navigate to the next page
                        window.location.href = 'score2.html';
                    });
                </script>
            </article>
            <article class="menu-item">
                <div id="container2">
                    <img src="img.jpeg" height="150px" width="150" alt="Click me" id="image2">
                </div>
                <h2>Image</h2>
                <script>
                    // JavaScript to handle the click event for Image
                    document.getElementById('image2').addEventListener('click', function() {
                        // Navigate to the next page
                        window.location.href = 'round2.html';
                    });
                </script>
            </article>
            <article class="menu-item">
                <div id="container3">
                    <img src="doc.jpeg" height="150px" width="150" alt="Click me" id="image3">
                </div>
                <h2>Doctor Suggestion</h2>
                <script>
                    // JavaScript to handle the click event for Doctor Suggestion
                    document.getElementById('image3').addEventListener('click', function() {
                        // Navigate to the next page
                        window.location.href = 'therapist.html';
                    });
                </script>
            </article>
        </section>
    </main>
</body>
</html>
